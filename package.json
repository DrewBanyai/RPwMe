{
  "name": "electron-multiple-windows-demo",
  "description": "An example electron app showing how to manage multiple windows.",
  "version": "1.0.0-alpha",
  "author": "Nate Goldman <nate@ngoldman.me> (http://ngoldman.me/)",
  "bugs": {
    "url": "https://github.com/ngoldman/electron-multiple-windows-demo/issues"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.55",
    "@electron-forge/maker-deb": "^6.0.0-beta.55",
    "@electron-forge/maker-rpm": "^6.0.0-beta.55",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.55",
    "@electron-forge/maker-zip": "^6.0.0-beta.55",
    "electron-prebuilt": "^0.36.9",
    "standard": "^6.0.7"
  },
  "homepage": "https://github.com/ngoldman/electron-multiple-windows-demo#readme",
  "keywords": [
    "electron",
    "many",
    "multiple",
    "tutorial",
    "windows"
  ],
  "license": "ISC",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ngoldman/electron-multiple-windows-demo.git"
  },
  "scripts": {
    "start": "electron-forge start",
    "test": "standard",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_multiple_windows_demo"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
