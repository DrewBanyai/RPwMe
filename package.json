{
  "name": "rpwme",
  "description": "RPwMe is a Twitch Chat integrated roleplay system for streamers and viewers to play together",
  "version": "0.0.1-alpha",
  "author": "DrewBanyai <drewbanyai@gmail.com> (https://github.com/DrewBanyai)",
  "bugs": {
    "url": "https://github.com/DrewBanyai/RPwMe/issues"
  },
  "dependencies": {
    "electron-squirrel-startup": "^1.0.0",
    "minimist": "^1.2.0"
  },
  "devDependencies": {
    "@electron-forge/cli": "^6.0.0-beta.55",
    "@electron-forge/maker-deb": "^6.0.0-beta.55",
    "@electron-forge/maker-rpm": "^6.0.0-beta.55",
    "@electron-forge/maker-squirrel": "^6.0.0-beta.55",
    "@electron-forge/maker-zip": "^6.0.0-beta.55",
    "electron-prebuilt": "^0.36.9",
    "standard": "^6.0.7"
  },
  "homepage": "https://github.com/DrewBanyai/RPwMe#readme",
  "keywords": [
    "rpwme",
    "roleplay",
    "twitch",
    "rpg",
    "twitchplays"
  ],
  "license": "See license at LICENSE.md",
  "main": "index.js",
  "private": true,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DrewBanyai/RPwMe"
  },
  "scripts": {
    "start": "electron-forge start",
    "test": "standard",
    "package": "electron-forge package",
    "make": "electron-forge make"
  },
  "config": {
    "forge": {
      "packagerConfig": {},
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {
            "name": "electron_multiple_windows_demo"
          }
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ]
    }
  }
}
